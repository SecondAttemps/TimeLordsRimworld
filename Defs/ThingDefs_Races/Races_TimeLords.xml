<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BasePawn">
    <defName>Time Lords</defName>
    <label>time lord</label>
    <description>A Time Lord, one of the oldest races in the universe. While outwardly human, their apperence belies remarkable differences from humanity.</description>
    <statBases>
      <MarketValue>4000</MarketValue>
      <MoveSpeed>6.42</MoveSpeed>
      <Flammability>0.5</Flammability>
      <ComfyTemperatureMin>-30</ComfyTemperatureMin>
      <ComfyTemperatureMax>60</ComfyTemperatureMax>
      <LeatherAmount>25</LeatherAmount>
    </statBases>
	<verbs>
      <li>
        <verbClass>Verb_MeleeAttack</verbClass>
        <defaultCooldownTime>1.45</defaultCooldownTime>
        <meleeDamageBaseAmount>10</meleeDamageBaseAmount>
        <meleeDamageDef>Blunt</meleeDamageDef>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
      </li>
      <li>
        <verbClass>Verb_MeleeAttack</verbClass>
        <defaultCooldownTime>1.45</defaultCooldownTime>
        <meleeDamageBaseAmount>15</meleeDamageBaseAmount>
        <meleeDamageDef>Blunt</meleeDamageDef>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
      </li>
    </verbs>
    <tools>
      <li>
        <label>left fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>7</power>
        <cooldownTime>1.6</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>22</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>7</power>
        <cooldownTime>1.6</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>22</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>5</power>
        <cooldownTime>1.8</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <commonality>0.2</commonality>
      </li>
    </tools>
    <race>
      <thinkTreeMain>Humanlike</thinkTreeMain>
      <thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
      <intelligence>Humanlike</intelligence>
      <makesFootprints>true</makesFootprints>
      <lifeExpectancy>400</lifeExpectancy>
      <leatherColor>(211,194,143)</leatherColor>
      <leatherCommonalityFactor>0.001</leatherCommonalityFactor>
      <leatherInsulation>0.70</leatherInsulation>
      <leatherMarketValueFactor>3</leatherMarketValueFactor>
	  <nameGenerator>NamerPersonTimelordM</nameGenerator>
      <nameGeneratorFemale>NamerPersonTimelordF</nameGeneratorFemale>
      <body>Timelord_body</body>
      <baseBodySize>1.25</baseBodySize>
      <baseHealthScale>1</baseHealthScale>
      <foodType>OmnivoreHuman</foodType>
      <gestationPeriodDays>45</gestationPeriodDays>
	  <lifeStageAges>
        <li>
          <def>BabyTimelordStage</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>ChildTimelordStage</def>
          <minAge>50</minAge>
        </li>
		<li>
          <def>TeenTimelordStage</def>
          <minAge>100</minAge>
        </li>
        <li>
          <def>AdultTimelordStage</def>
          <minAge>150</minAge>
        </li>
        <li>
          <def>ElderTimelordStage</def>
          <minAge>5000</minAge>
        </li>
        <li>
          <def>AncestorTimelordStage</def>
          <minAge>10000</minAge>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_Punch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_Punch_Miss</soundMeleeMiss>
      <specialShadowData>
        <volume>(0.3, 0.8, 0.4)</volume>
        <offset>(0,0,-0.3)</offset>
      </specialShadowData>
      <ageGenerationCurve>
        <points>
          <li>(14,0)</li>
          <li>(16,100)</li>
          <li>(50,100)</li>
          <li>(60,30)</li>
          <li>(70,18)</li>
          <li>(80,10)</li>
          <li>(90,3)</li>
          <li>(100,0)</li>
        </points>
      </ageGenerationCurve>
      <hediffGiverSets>
        <li>OrganicStandard</li>
      </hediffGiverSets>
    </race>
    <recipes>
      <li>InstallPowerClaw</li>
      <li>InstallScytherBlade</li>
      <li>InstallBionicEye</li>
      <li>InstallBionicArm</li>
      <li>InstallBionicLeg</li>
      <li>InstallSimpleProstheticArm</li>
      <li>InstallSimpleProstheticLeg</li>
      <li>InstallPegLeg</li>
      <li>InstallDenture</li>
      <li>InstallJoywire</li>
      <li>InstallPainstopper</li>
      <li>ExciseCarcinoma</li>
      <li>RemoveBodyPart</li>
      <li>Euthanize</li>
    </recipes>
  </ThingDef>

</Defs>
